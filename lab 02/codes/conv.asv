function y = conv(x,h)
a = length(x);
b = length(h);
n = a+b-1;   %output comes out from 0 to a+b-2
y = zeros(1,n);%initialization to the zero
l = 1:n;
for i = 0:n
            for j = 0:n
                        if ((i-j+1)>0 && (i-j+1)<=b && (j+1)<=a)
                            print();
                            y(i+1) = y(i+1)+ x(j+1).*h(i-j+1);
                         end
            end
end
disp(y)
stem(l,y)
